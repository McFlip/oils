{
  "name": "oils",
  "version": "1.0.0",
  "description": "YL oils database",
  "main": "index.js",
  "repository": "git@github.com:McFlip/oils.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --host 0.0.0.0",
    "test": "jest"
  },
  "author": "Grady Denton",
  "license": "ISC",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/reactTests/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "components"
    ],
    "moduleNameMapper": {
      "^components/(.*)": "<rootDir>/src/components/$1",
      "^reducers/(.*)": "<rootDir>/src/reducers/$1",
      "^actions/(.*)": "<rootDir>/src/actions/$1",
      "^constants/(.*)": "<rootDir>/src/constants/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/reactTests/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/reactTests/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.7.1",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.3.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-form": "^7.4.12",
    "@types/redux-promise": "^0.5.28",
    "acorn": "^6.1.1",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "dompurify": "^1.0.10",
    "dotenv": "^8.0.0",
    "immer": "^2.1.5",
    "jquery": "^3.4.0",
    "lodash": "^4.17.11",
    "marked": "^0.6.2",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.0.0",
    "redux": "^3.0.4",
    "redux-form": "^7.4.2",
    "redux-promise": "^0.5.3"
  }
}
