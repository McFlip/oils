{
  "name": "oils",
  "version": "1.0.0",
  "description": "YL oils database",
  "main": "index.js",
  "repository": "git@github.com:McFlip/oils.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --host 0.0.0.0",
    "test": "jest"
  },
  "author": "Grady Denton",
  "license": "ISC",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/reactTests/setupTests.js",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "components"
    ],
    "moduleNameMapper": {
      "^components/(.*)": "<rootDir>/src/components/$1",
      "^reducers/(.*)": "<rootDir>/src/reducers/$1",
      "^actions/(.*)": "<rootDir>/src/actions/$1",
      "^constants/(.*)": "<rootDir>/src/constants/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/reactTests/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/reactTests/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.1",
    "react-testing-library": "^5.2.3",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.0.0",
    "redux": "^3.0.4",
    "redux-form": "^7.4.2",
    "redux-promise": "^0.5.3"
  }
}
