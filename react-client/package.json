{
  "name": "oils",
  "version": "1.0.0",
  "description": "YL oils database",
  "main": "index.js",
  "repository": "git@github.com:McFlip/oils.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --host 0.0.0.0",
    "test": "jest",
    "build": "webpack --config webpack.config.production.js && cp -r index.html favicon.ico style ./dist/"
  },
  "author": "Grady Denton",
  "license": "ISC",
  "browserslist": [
    "last 2 versions",
    "> 5% in US",
    "Firefox ESR",
    "not dead"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/reactTests/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "components"
    ],
    "moduleNameMapper": {
      "^components/(.*)": "<rootDir>/src/components/$1",
      "^reducers/(.*)": "<rootDir>/src/reducers/$1",
      "^actions/(.*)": "<rootDir>/src/actions/$1",
      "^constants/(.*)": "<rootDir>/src/constants/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/reactTests/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/reactTests/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^8.0.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^6.0.10",
    "@types/react-router-dom": "^4.3.5",
    "@types/redux-form": "^7.5.7",
    "@types/redux-promise": "^0.5.28",
    "acorn": "^6.4.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "dompurify": "^2.0.7",
    "dotenv": "^8.2.0",
    "immer": "^2.1.5",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "marked": "^0.7.0",
    "popper.js": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^5.1.2",
    "react-router-dom": "^4.0.0",
    "redux": "^3.0.4",
    "redux-form": "^7.4.2",
    "redux-promise": "^0.5.3"
  }
}
